// pyschool-frontend/prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// --- AGREGA ESTE MODELO DUMMY PARA QUE EL BUILD PASE ---
model BuildFix {
  id    Int     @id @default(autoincrement())
  name  String
}